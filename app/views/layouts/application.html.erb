<!DOCTYPE html>
<html>
<head>
  <title><%= yield(:title) || "Untitled" %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
<%= yield(:head) %>
</head>
<body>
	<div id="container">
	      <div id="user_nav">
				<% if user_signed_in? %>
					Signed in as <%= current_user.email %>. Not you?
					<%= link_to "Sign out", destroy_user_session_path %>
				<% else %>
					<%= link_to "Sign up", new_user_registration_path %> or <%= link_to "sign in", new_user_session_path %><br/>
					<%= link_to_rpx "sign in To yahoo,gmail", user_session_url %>
				<% end %>
		  </div>
		  <div >
			    <ul class="hmenu">
				  <%= render 'devise/menu/registration_items' %>
				  <%= render 'devise/menu/login_items' %>
				</ul>
		  </div>
     <%= yield %>
</div>

<%= javascript_include_rpx(user_session_url) %>

</body>
</html>
